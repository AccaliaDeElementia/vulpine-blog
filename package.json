{
  "name": "vulpine-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "copyfilessrcdist": "copyfiles -u 1 \"./src/public/**/*\" \"./src/views/**/*.pug\" \"./dist\"",
    "precopyfilessrcdist": "npm run copyfilespkgjs",
    "copyfilespkgjs": "copyfiles -u 1 \"node_modules/jquery/dist/**/*\" \"node_modules/bootstrap/dist/js/**/*\" \"node_modules/popper.js/dist/**/*\" \"./dist/public/javascripts\"",
    "precopyfilespkgjs": "npm run copyfilespkgcss",
    "copyfilespkgcss": "copyfiles -u 1 \"node_modules/bootswatch/**/*.scss\" \"node_modules/bootstrap/**/*.scss\" \"./dist/public/stylesheets\"",
    "test": "eslint src --ext .ts --ext .js --ext .jsx",
    "prebuild": "npm test",
    "build": "tsc",
    "postbuild": "npm run copyfilessrcdist",
    "prestart": "npm run build",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "@types/commonmark": "^0.27.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.4",
    "@types/node-sass-middleware": "0.0.31",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "commonmark": "^0.29.1",
    "cookie-parser": "^1.4.4",
    "copyfiles": "^2.2.0",
    "debug": "^2.6.9",
    "eslint": "^6.8.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "express": "^4.16.4",
    "http-errors": "^1.6.3",
    "jquery": "^3.5.0",
    "morgan": "^1.9.1",
    "node-sass-middleware": "^0.11.0",
    "popper.js": "^1.16.1",
    "pug": "^2.0.4",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  },
  "devDependencies": {}
}
